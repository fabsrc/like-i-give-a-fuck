{
 "manifest_version": 2,
 "name": "Like I Give a Fuck",
 "description": "Changes every 'Like' button and text field to 'Give a fuck' on Facebook",
 "version": "1.1.2",
 "icons": {
  "16": "icons/icon16.png",
  "32": "icons/icon32.png",
  "96": "icons/icon96.png",
  "128": "icons/icon128.png",
  "256": "icons/icon256.png",
  "512": "icons/icon512.png"
 },
 "content_scripts": [{
  "matches": [
   "*://*.facebook.com/*"
  ],
  "js": [
   "main.js"
  ]
 }]
}